<% GamePlayer::COLORS.each do |color| %>
$<%= color.downcase %>: <%= GamePlayer.hex_color(color) %>;
<% end %>

#player-profile {
  .site, .location{
    margin-top: 10px;
  }
  .message-link{
    float: right;
    margin-top: 20px;
    a{
      float: right;
    }
  }
}

.nearby-players{
  tr{
    height: 35px;
  }
}

.red{
  color: $red;
}

.blue{
  color: $blue;
}

.green{
  color: $green;
}

.black{
  color: $black;
}

.gold{
  color: $gold;
}
